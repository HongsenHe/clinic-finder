<h1>Listing clinics</h1>

<table>
  <thead>
    <tr>
      <th>Street</th>
      <th>Street optional</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @clinics.each do |clinic| %>
      <tr>
        <td><%= clinic.street %></td>
        <td><%= clinic.street_optional %></td>
        <td><%= clinic.city %></td>
        <td><%= clinic.state %></td>
        <td><%= clinic.zip %></td>
        <td><%= link_to 'Show', clinic %></td>
        <td><%= link_to 'Edit', edit_clinic_path(clinic) %></td>
        <td><%= link_to 'Destroy', clinic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clinic', new_clinic_path %>
